            {{ plugin_list_name }}PluginListComponent(parent: parent)
        } with: {
            // swiftlint:disable:next direct_return
            let dependency: {{ plugin_list_name }}PluginListDependencyMock = .init()
            // dependency.<dependencyName> = <dependencyName>Mock
            return dependency
        })
    }

